    }
}

if (walls[2]) { // draw bottom wall
    endcounter++;
    window = -1
    if (irandom(2) % 2 == 0){
        window = floor(random_range(2, w / 32 - 2))
    }
    for (i = 0; i &lt; (w / 32); i++) {
        instance_create(x + i*32, y + w - 32, obj_wall);
