    }
    else if (right &gt; 0){
        draw_sprite_ext(spr_cup_right, right_count, 20, window_get_height() - 200, 4.0, 4.0, 0, c_white, 1)
        if (even_counter % 4 == 0) right_count++
        if (right_count &gt; 2) right_count = 0;
        even_counter++
    }
    else {
        draw_sprite_ext(spr_cup, 0, 20, window_get_height() - 200, 4.0, 4.0, 0, c_white, 1)
    }
}
