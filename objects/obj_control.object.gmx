        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Lighting
surface_set_target(light);
//HEX is BGR for god know's why
draw_set_color($666666);
draw_rectangle(0, 0, view_wview, view_hview, false);
surface_reset_target();


if (!paused){
    if (instance_number(obj_window)){
        //lightning
        rain_counter -= 1;
        
        //show_debug_message(rain_counter);
        if (rain_counter &lt; 0){
            rain_counter = random(750);
            if (obj_window.sprite_index == spr_window_rain ) {
                obj_window.sprite_index = spr_window_lightning;
            }
        }
        if (obj_window.sprite_index == spr_window_lightning &amp;&amp; obj_window.image_index == image_number){
            obj_window.sprite_index = spr_window_rain;
        }
    }
}

if (keyboard_check(global.pause) &amp;&amp; !pause_held_down){
    pause_held_down = true;
    if (!paused){
        paused = true;
        global.pauseSurface = screenshot();
        instance_deactivate_all(true);
        instance_create(0,0,obj_pause);
    } else {
        instance_activate_all();
        //sprite_delete(global.pauseSurface);
        with(obj_pause){
            instance_destroy();
        }
        paused = false;
    }
} else {
    if (!keyboard_check(global.pause))
        pause_held_down = false;
}
</string>
