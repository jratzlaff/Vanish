    } else {
        draw_sprite_ext(spr_heart, 0, 105, window_get_height() - 90, 4.0, 4.0, 0, c_white, 1)
    }
    
    var left = instance_number(obj_portal_left)
    var right = instance_number(obj_portal_right)
    
    if (left &gt; 0 &amp;&amp; right &gt; 0){
       draw_sprite_ext(spr_swirl_both, 0, 10, window_get_height() - 115, 4.0, 4.0, 0, c_white, 1) 
    }
    else if (left &gt; 0){
        draw_sprite_ext(spr_cup_left, left_count, 20, window_get_height() - 200, 4.0, 4.0, 0, c_white, 1)
        if (even_counter % 4 == 0) left_count++
