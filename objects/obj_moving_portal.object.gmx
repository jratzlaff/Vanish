y = y - wall;

if (sprite_index == spr_moving_right_portal){
    with(obj_portal_right) {
        instance_destroy();
    }
    var portal = instance_create(x+x_change,y+y_change,obj_portal_right);
    portal.image_angle = angle;
} else {
    with(obj_portal_left) {
        instance_destroy()
    }
    var portal = instance_create(x+x_change,y+y_change,obj_portal_left);
    portal.image_angle = angle;
}

instance_destroy();


